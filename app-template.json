    "repoBranch": "refs/heads/master",
    "image": "public/img/cloud_plugins/sharepoint.png",
    "category": "SaaS Connectors",
    "configuration": [
        {
            "desc": "Your Sharepoint Login",
            "varName": "SHAREPOINT_LOGIN"
